<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hungtran.bankingassistant">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:name=".ui.BankingApplication"
        tools:ignore="GoogleAppIndexingWarning">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.map.MapActivity" />
        <activity android:name=".ui.login.LoginActivty" />
        <!--<activity android:name=".ui.splash.SplashActivity"/>-->
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.main.MainActivityNew"/>
        <activity android:name=".ui.wallet.WalletActivity"/>
        <activity android:name=".ui.linkingBankMockup.LinkingBankMockupActivity"/>
        <activity android:name=".ui.AvaibleBankLinkingAcitivity.AvaiableBankLinkingActivity"/>
        <activity android:name=".ui.myAccountCardList.MyAccountCardListActivity"/>
        <activity android:name=".ui.detailAccount.DetailATMAccountActivity"/>
        <activity android:name=".ui.createSavingAccount.CreateSavingAccountActivity"/>
        <activity android:name=".ui.pressOTP.OTPAcvitiy"/>
        <activity android:name=".ui.transferMoneyATM.TransferMoneyATMActivity"/>
        <activity android:name=".ui.calculator.CalculatorActivity"/>
        <activity android:name=".ui.detailAccount.DetailSavingAccountActivity"/>
        <activity android:name=".ui.transferMoneyATM.TransferMoneySuccessAcitvity"/>
        <activity android:name=".ui.withdrawMoney.WithdrawMoneyActivity"/>
        <activity android:name=".ui.changeSavingMoneyBank.ChangeSavingMoneyBankActivity"/>
        <activity android:name=".ui.withdrawMoney.WithDrawAllMoneyActivity"/>
        <activity android:name=".ui.transactionHistory.TransactionHistoryActivity"/>
        <activity android:name=".ui.transactionHistory.TransactionHistoryDetailAcitivty"/>
        <activity android:name=".ui.register.RegisterActivity"/>
        <activity android:name=".ui.changePassword.ChangePasswordAcitvity"/>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_map_api_key" />

        <service
            android:name=".service.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>